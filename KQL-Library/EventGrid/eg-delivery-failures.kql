union isfuzzy=true EventGridDeliveryFailures | project TimeGenerated, TopicResourceId, EventSubscriptionId, DeliveryAttempt, ErrorType, ErrorMessage | union (AzureDiagnostics | where Category has 'EventGrid' and Message has 'Delivery' and Message has_any ('Failed','Error') | project TimeGenerated, TopicResourceId=resourceId, EventSubscriptionId=eventSubscriptionId_s, DeliveryAttempt=deliveryAttempt_d, ErrorType=errorType_s, ErrorMessage=message) | order by TimeGenerated desc
