union isfuzzy=true DnsQueryLogs | project TimeGenerated, ClientIP, QueryName, QueryType, ResponseCode | union (AzureDiagnostics | where Category has 'Dns' or Category has 'DNS' | project TimeGenerated, ClientIP=clientIp_s, QueryName=queryName_s, QueryType=queryType_s, ResponseCode=responseCode_s) | summarize Queries=count() by QueryName, QueryType, ResponseCode | top 200 by Queries desc
